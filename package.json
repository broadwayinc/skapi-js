{
  "name": "skapi-js",
  "version": "1.2.13-beta.0",
  "description": "Serverless Backend API for HTML frontend.",
  "main": "dist/skapi.cjs",
  "browser": "dist/skapi.js",
  "module": "dist/skapi.mjs",
  "unpkg": "dist/skapi.js",
  "jsdelivr": "dist/skapi.js",
  "types": "js/Main.d.ts",
  "exports": {
    ".": {
      "types": "./js/Main.d.ts",
      "browser": "./dist/skapi.js",
      "node": {
        "import": "./dist/skapi.node.mjs",
        "require": "./dist/skapi.cjs"
      },
      "import": "./dist/skapi.mjs",
      "require": "./dist/skapi.cjs",
      "default": "./dist/skapi.cjs"
    }
  },
  "files": [
    "js",
    "dist"
  ],
  "scripts": {
    "build": "npx tsc --project tsconfig.json && npx webpack --config webpack.config.js",
    "dev": "npx bns 3333"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broadwayinc/skapi-js.git"
  },
  "author": "Baksa Gimm",
  "copyright.owner": "broadway Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/broadwayinc/skapi-js/issues"
  },
  "homepage": "https://www.skapi.com",
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.12",
    "qpass": "^1.0.0"
  },
  "devDependencies": {
    "basic-node-server": "^1.0.16",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "backend",
    "skapi",
    "api",
    "serverless",
    "database"
  ]
}
