{
  "name": "skapi-js",
  "version": "1.0.188-beta.1",
  "description": "Skapi: Backend API for HTML frontend.",
  "main": "js/Main.js",
  "browser": "dist/skapi.js",
  "types": "js/Main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/skapi.cjs",
      "import": "./js/Main.js",
      "types": "./js/Main.d.ts"
    }
  },
  "files": [
    "js",
    "dist"
  ],
  "scripts": {
    "build": "npx tsc --project tsconfig.json && npx webpack --config webpack.config.js && cp dist/skapi.js sandbox/skapi.js",
    "dev": "cd sandbox && npx bns 3300"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broadwayinc/skapi-js.git"
  },
  "author": "Baksa Gimm",
  "copyright.owner": "broadway Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/broadwayinc/skapi-js/issues"
  },
  "homepage": "https://www.skapi.com",
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.12",
    "basic-node-server": "^1.0.11"
  },
  "devDependencies": {
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "backend",
    "skapi",
    "api",
    "daas",
    "baas",
    "serverless",
    "server",
    "database"
  ]
